@model IEnumerable<WebApplication45.Model.employee>

<table class="table table-bordered">
    <tr>
        <th>id</th>
        <th>name</th>
        <th>email</th>
        <th>salary</th>
        <th>gender</th>

    </tr>
    @foreach (var emp in Model)
    {
        <tr>
            <td>@emp.id</td>
            <td>@emp.name</td>
            <td>@emp.email</td>
            <td>@emp.salary</td>
            <td>@emp.gender</td>
        </tr>
    }




</table>

